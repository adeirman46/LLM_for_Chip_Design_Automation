    .port_info 0 /INPUT 32 "a";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /INPUT 32 "x";
    .port_info 3 /INPUT 32 "b";
    .port_info 4 /OUTPUT 65 "y";
P_0x6326ab51f520 .param/l "width" 0 3 2, +C4<00000000000000000000000000100000>;
v0x6326ab55aec0_0 .net "a", 31 0, v0x6326ab56d5f0_0;  1 drivers
v0x6326ab56d190_0 .net "b", 31 0, v0x6326ab56d6d0_0;  1 drivers
v0x6326ab56d270_0 .net "clk", 0 0, v0x6326ab56d7a0_0;  1 drivers
v0x6326ab56d340_0 .net "x", 31 0, v0x6326ab56d8a0_0;  1 drivers
v0x6326ab56d420_0 .var "y", 64 0;
E_0x6326ab55bc30 .event posedge, v0x6326ab56d270_0;
    .scope S_0x6326ab51efa0;
T_0 ;
    %wait E_0x6326ab55bc30;
    %load/vec4 v0x6326ab55aec0_0;
    %pad/u 65;
    %load/vec4 v0x6326ab56d340_0;
    %pad/u 65;
    %mul;
    %load/vec4 v0x6326ab56d190_0;
    %pad/u 65;
    %add;
    %assign/vec4 v0x6326ab56d420_0, 0;
    %jmp T_0;
    .thread T_0;
    .scope S_0x6326ab51edc0;
T_1 ;
    %vpi_call 2 24 "$dumpfile", "mac_module.vcd" {0 0 0};
    %vpi_call 2 25 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x6326ab51edc0 {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x6326ab51edc0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6326ab56d7a0_0, 0, 1;
T_2.0 ;
    %delay 50, 0;
    %load/vec4 v0x6326ab56d7a0_0;
    %inv;
    %store/vec4 v0x6326ab56d7a0_0, 0, 1;
    %jmp T_2.0;
    %end;
    .thread T_2;
    .scope S_0x6326ab51edc0;
T_3 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x6326ab56d5f0_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x6326ab56d8a0_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x6326ab56d6d0_0, 0, 32;
    %delay 100, 0;
    %pushi/vec4 1, 0, 32;
    %store/vec4 v0x6326ab56d5f0_0, 0, 32;
    %pushi/vec4 2, 0, 32;
    %store/vec4 v0x6326ab56d8a0_0, 0, 32;
    %pushi/vec4 3, 0, 32;
    %store/vec4 v0x6326ab56d6d0_0, 0, 32;
    %delay 100, 0;
    %vpi_call 2 42 "$finish" {0 0 0};
    %end;
