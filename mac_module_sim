v0x6326ab56d6d0_0 .var "b", 31 0;
v0x6326ab56d7a0_0 .var "clk", 0 0;
v0x6326ab56d8a0_0 .var "x", 31 0;
v0x6326ab56d970_0 .net "y", 64 0, v0x6326ab56d420_0;  1 drivers
