:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x566ab0511dc0 .scope module, "adder_tb" "adder_tb" 2 3;
 .timescale -9 -10;
P_0x566ab0512ea0 .param/l "width" 1 2 5, +C4<00000000000000000000000000100000>;
v0x566ab0560080_0 .var "A", 31 0;
v0x566ab0560160_0 .var "B", 31 0;
v0x566ab0560230_0 .net "Y", 32 0, v0x566ab055fe20_0;  1 drivers
v0x566ab0560330_0 .var "clk", 0 0;
S_0x566ab0511fa0 .scope module, "uut" "adder" 2 14, 3 1 0, S_0x566ab0511dc0;
 .timescale -9 -10;
    .port_info 0 /INPUT 32 "A";
    .port_info 1 /INPUT 32 "B";
    .port_info 2 /INPUT 1 "clk";
    .port_info 3 /OUTPUT 33 "Y";
P_0x566ab0512500 .param/l "width" 0 3 2, +C4<00000000000000000000000000100000>;
v0x566ab054e170_0 .net "A", 31 0, v0x566ab0560080_0;  1 drivers
v0x566ab055fd40_0 .net "B", 31 0, v0x566ab0560160_0;  1 drivers
v0x566ab055fe20_0 .var "Y", 32 0;
v0x566ab055ff10_0 .net "clk", 0 0, v0x566ab0560330_0;  1 drivers
E_0x566ab054f1f0 .event posedge, v0x566ab055ff10_0;
    .scope S_0x566ab0511fa0;
T_0 ;
    %wait E_0x566ab054f1f0;
    %load/vec4 v0x566ab054e170_0;
    %pad/u 33;
    %load/vec4 v0x566ab055fd40_0;
    %pad/u 33;
    %add;
    %assign/vec4 v0x566ab055fe20_0, 0;
    %jmp T_0;
    .thread T_0;
    .scope S_0x566ab0511dc0;
T_1 ;
    %vpi_call 2 22 "$dumpfile", "adder.vcd" {0 0 0};
    %vpi_call 2 23 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x566ab0511dc0 {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x566ab0511dc0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x566ab0560330_0, 0, 1;
T_2.0 ;
    %delay 50, 0;
    %load/vec4 v0x566ab0560330_0;
    %inv;
    %store/vec4 v0x566ab0560330_0, 0, 1;
    %jmp T_2.0;
    %end;
    .thread T_2;
    .scope S_0x566ab0511dc0;
T_3 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x566ab0560080_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x566ab0560160_0, 0, 32;
    %delay 100, 0;
    %pushi/vec4 1, 0, 32;
    %store/vec4 v0x566ab0560080_0, 0, 32;
    %pushi/vec4 1, 0, 32;
    %store/vec4 v0x566ab0560160_0, 0, 32;
    %delay 100, 0;
    %pushi/vec4 2, 0, 32;
    %store/vec4 v0x566ab0560080_0, 0, 32;
    %pushi/vec4 3, 0, 32;
    %store/vec4 v0x566ab0560160_0, 0, 32;
    %delay 100, 0;
    %pushi/vec4 4, 0, 32;
    %store/vec4 v0x566ab0560080_0, 0, 32;
    %pushi/vec4 5, 0, 32;
    %store/vec4 v0x566ab0560160_0, 0, 32;
    %delay 100, 0;
    %vpi_call 2 39 "$finish" {0 0 0};
    %end;
    .thread T_3;
